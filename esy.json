{
  "name": "fullstack-reason",
  "version": "0.5.0",
  "description": "Example Reason Esy Project",
  "license": "MIT",
  "esy": {
    "build": "dune build --only-packages #{self.name}",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "bin": [
        "App"
      ]
    },
    "buildEnv": {
      "ODOC_SYNTAX": "re"
    }
  },
  "scripts": {
    "bin": "esy b dune exec backend/App.bc",
    "doc": "esy dune build @doc"
  },
  "dependencies": {
    "@esy-ocaml/reason": ">= 3.6.0 < 4.0.0",
    "@opam/dune": "< 3.0.0",
    "@opam/lwt": ">= 5.3.0 < 6.0.0",
    "@opam/ppx_deriving_yojson": ">= 3.5.1 < 4.0.0",
    "@opam/re-web": "*",
    "ocaml": "~4.8.1000"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "ocaml": "~4.8.1000",
    "@opam/odoc": "*",
    "@opam/utop": "*"
  },
  "resolutions": {
    "@opam/re-web": "yawaramin/re-web:package.json#ddcb52f"
  }
}
